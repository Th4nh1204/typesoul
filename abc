local Main = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Main:AddSlider({
	Name = "Walk Speed",
	Min = 0,
	Max = 200,
	Default = 20,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
	Callback = function(Value)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end    
})

Main:AddToggle({
	Name = "Walk Speed Toggle | Default 150 Speed",
	Default = false,
	Callback = function(Value)
        getgenv().speed = Value
        if getgenv().speed == true then
            while wait(0.1) do
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 150
            end
	end
end 
})

Main:AddToggle({
	Name = "Turn OFF Walk Speed Toggle",
	Default = false,
	Callback = function(Value)
        getgenv().speed2 = Value
        if getgenv().speed2 == true then
            getgenv().speed = false
            while wait(0.1) do
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 15
            end
	end
end 
})

Main:AddButton({
	Name = "Teleport To Harribel NPC",
	Callback = function()
        game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(game:GetService("Workspace").NPCs.PartialRes.PartialRes.HumanoidRootPart.CFrame)
    end       
})

Main:AddButton({
	Name = "FlySpeed (Press RightControl | Default 200 Speed)",
	Callback = function()
        _G.flyspeed = 200
        _G.on = false
        
        local UserInputService = game:GetService("UserInputService")
        local RunService = game:GetService("RunService")
        
        local root = game.Players.LocalPlayer.Character.PrimaryPart
        
        UserInputService.InputBegan:Connect(function(inputObject, processed)
            if inputObject.KeyCode == Enum.KeyCode.LeftControl then
                _G.on = not _G.on
            end
        end)
        
        RunService.RenderStepped:Connect(function()
            if _G.on then
                root.Velocity = root.CFrame.LookVector * _G.flyspeed
            end
        end)
  	end    
})
